import RafManifestYml
pipeline {
    agent {
		label 'yml_node'
	}
	parameters {
        string(
            defaultValue: 'raf_fullgression_F_225.yml',
            description: "Manifest file name",
            name: 'MANIFEST_NAME')
    }
    stages {
        stage('Example Build') {
            options{ 
                        timeout(time: 4, unit: 'SECONDS')
                    }
            steps {
                echo 'Hello, Maven'
            }
        }
        stage('Example Test') {
            steps {
                echo 'Hello, JDK'
            }
        }
    }
}